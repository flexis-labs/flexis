{
    "name": "flexis-labs/flexis",
    "type": "library",
    "description": "The Flexis PHP framework",
    "keywords": [
        "framework",
        "dev"
    ],
    "license": "MIT",
    "require": {
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "ext-zip": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-curl": "*",
        "symfony/yaml": "^7.1",
        "psr/container": "^2.0",
        "psr/log": "^3.0",
        "psr/http-message": "^2.0",
        "psr/http-client": "^1.0",
        "laminas/laminas-diactoros": "^3.5",
        "composer/ca-bundle": "^1.5",
        "opis/closure": "^3.6",
        "symfony/console": "^7.1",
        "symfony/error-handler": "^7.1",
        "defuse/php-encryption": "^2.4",
        "paragonie/sodium_compat": "^2.1",
        "algo26-matthias/idna-convert": "^4.0",
        "doctrine/inflector": "^2.0"
    },
    "require-dev": {
        "ext-sqlsrv": "*",
        "ext-mysqli": "*"
    },
    "autoload": {
        "psr-4": {
            "Flexis\\Application\\": "src/application/",
            "Flexis\\Archive\\": "src/archive/",
            "Flexis\\Authentication\\": "src/authentication/",
            "Flexis\\Console\\": "src/console/",
            "Flexis\\Crypt\\": "src/crypt/",
            "Flexis\\Data\\": "src/data/",
            "Flexis\\Database\\": "src/database/",
            "Flexis\\DIP\\": "src/dip/",
            "Flexis\\Event\\": "src/event/",
            "Flexis\\Filesystem\\": "src/filesystem/",
            "Flexis\\Filter\\": "src/filter/",
            "Flexis\\Http\\": "src/http/",
            "Flexis\\Image\\": "src/image/",
            "Flexis\\Input\\": "src/input/",
            "Flexis\\Language\\": "src/language/",
            "Flexis\\Microdata\\": "src/microdata/",
            "Flexis\\Profiler\\": "src/profiler/",
            "Flexis\\Proxy\\": "src/proxy/",
            "Flexis\\Registry\\": "src/registry/",
            "Flexis\\Router\\": "src/router/",
            "Flexis\\Session\\": "src/session/",
            "Flexis\\String\\": "src/string/",
            "Flexis\\Uri\\": "src/uri/",
            "Flexis\\Utilities\\": "src/utilities/"
        }
    }
}
